net: "<PROTO_PATH>/train_val.prototxt"

# number of iterations per test
test_iter: 50

# interval between tests
test_interval: 250

base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 2000
display: 100
max_iter: 2500
momentum: 0.9
weight_decay: 0.0005
snapshot: 2500
snapshot_prefix: "snapshots/ONE-<NET_NUMBER>"
solver_mode: GPU
